<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Theme 1--> 

    <!--Background Gradient Colors--> 
    <Color x:Key="BackgroundGradientColor1">#9A3B3B</Color>
    <Color x:Key="BackgroundGradientColor2">#AE4F4F</Color>

    <!--Foreground Gradient Colors--> 
    <Color x:Key="ForegroundGradientColor1">#C08261</Color>
    <Color x:Key="ForegroundGradientColor2">#D49675</Color>

    <!--Detail Gradient Colors-->
    <Color x:Key="DetailGradientColor1">#E2C799</Color>
    <Color x:Key="DetailGradientColor2">#F6DBAD</Color>

    <!--Text Gradient Colors-->
    <Color x:Key="TextColor">#FCFBED</Color>
    <Color x:Key="DarkerTextColor">#FF4A4A4A</Color>
    <SolidColorBrush x:Key="DarkerTextColorBrush" Color="#FF4A4A4A"/>
    
    <!--Button Colors-->
    <Color x:Key="ButtonNormalGC1">#BD5E5E</Color>
    <SolidColorBrush x:Key="ButtonNormalBrush" Color="#BD5E5E"/>
    
    <Color x:Key="ButtonNormalGC2">#BD5E5E</Color>

    <Color x:Key="ButtonHoveredOverGC1">#C08261</Color>
    <Color x:Key="ButtonHoveredOverGC2">#C08261</Color>

    <Color x:Key="ButtonSelectedGC1">#E2C799</Color>
    <Color x:Key="ButtonSelectedGC2">#E2C799</Color>

    <SolidColorBrush x:Key="ButtonHoveredBrush" Color="#E2C799"/>
    <SolidColorBrush x:Key="ButtonSelectedBrush" Color="#F6DBAD"/>




</ResourceDictionary>